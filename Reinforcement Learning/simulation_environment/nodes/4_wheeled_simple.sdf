<?xml version="1.0" ?>
<sdf version="1.7">
    <world name="car_world">
        <physics name="1ms" type="ignored">
            <max_step_size>0.001</max_step_size>
            <real_time_factor>1.0</real_time_factor>
        </physics>
        <plugin
            filename="ignition-gazebo-physics-system"
            name="ignition::gazebo::systems::Physics">
        </plugin>
        <plugin
      	    filename="ignition-gazebo-sensors-system"
      	    name="ignition::gazebo::systems::Sensors">
      	    <render_engine>ogre</render_engine>
    	</plugin>
        <plugin
            filename="ignition-gazebo-user-commands-system"
            name="ignition::gazebo::systems::UserCommands">
        </plugin>
        <plugin
            filename="ignition-gazebo-scene-broadcaster-system"
            name="ignition::gazebo::systems::SceneBroadcaster">
        </plugin>

        <gui fullscreen="0">

            <!-- 3D scene -->
            <plugin filename="GzScene3D" name="3D View">
                <ignition-gui>
                <title>3D View</title>
                <property type="bool" key="showTitleBar">false</property>
                <property type="string" key="state">docked</property>
                </ignition-gui>

                <engine>ogre2</engine>
                <scene>scene</scene>
                <ambient_light>0.4 0.4 0.4</ambient_light>
                <background_color>0.8 0.8 0.8</background_color>
            </plugin>

            <!-- World control -->
            <plugin filename="WorldControl" name="World control">
                <ignition-gui>
                <title>World control</title>
                <property type="bool" key="showTitleBar">false</property>
                <property type="bool" key="resizable">false</property>
                <property type="double" key="height">72</property>
                <property type="double" key="width">121</property>
                <property type="double" key="z">1</property>

                <property type="string" key="state">floating</property>
                <anchors target="3D View">
                    <line own="left" target="left"/>
                    <line own="bottom" target="bottom"/>
                </anchors>
                </ignition-gui>

                <play_pause>true</play_pause>
                <step>true</step>
                <start_paused>true</start_paused>
                <service>/world/car_world/control</service>
                <stats_topic>/world/car_world/stats</stats_topic>
            </plugin>

            <!-- World statistics -->
            <plugin filename="WorldStats" name="World stats">
                <ignition-gui>
                <title>World stats</title>
                <property type="bool" key="showTitleBar">false</property>
                <property type="bool" key="resizable">false</property>
                <property type="double" key="height">110</property>
                <property type="double" key="width">290</property>
                <property type="double" key="z">1</property>

                <property type="string" key="state">floating</property>
                <anchors target="3D View">
                    <line own="right" target="right"/>
                    <line own="bottom" target="bottom"/>
                </anchors>
                </ignition-gui>

                <sim_time>true</sim_time>
                <real_time>true</real_time>
                <real_time_factor>true</real_time_factor>
                <iterations>true</iterations>
                <topic>/world/car_world/stats</topic>

            </plugin>

            <!-- Entity tree -->
            <plugin filename="EntityTree" name="Entity tree">
            </plugin>
            
            <plugin filename="KeyPublisher" name="Key Publisher"/>
            
            <plugin filename="ImageDisplay" name="Image Display"/>

        </gui>

        <light type="directional" name="sun">
            <cast_shadows>true</cast_shadows>
            <pose>0 0 10 0 0 0</pose>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
            <attenuation>
                <range>1000</range>
                <constant>0.9</constant>
                <linear>0.01</linear>
                <quadratic>0.001</quadratic>
            </attenuation>
            <direction>-0.5 0.1 -0.9</direction>
        </light>

        <model name="ground_plane">
            <static>true</static>
            <link name="link">
                <collision name="collision">
                <geometry>
                    <plane>
                    <normal>0 0 1</normal>
                    </plane>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.7</mu>
                            <mu2>0.7</mu2>
                        </ode>
                    </friction>
                </surface>
                </collision>
            </link>
        </model>

        <model name='vehicle_blue' canonical_link='chassis'>
            <pose relative_to='world'>0 0 0 0 0 0</pose>   <!--the pose is relative to the world by default-->
            
            <!--chassis-->
            <link name='chassis'>   
                <pose relative_to='__model__'>0.5 0 1.2 0 0 0</pose> 
                <inertial> <!--inertial properties of the link mass, inertia matix-->
                    <mass>4.14395</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>2.0 1.0 0.5</size> 
                        </box>
                    </geometry>
                    <!--Color of the link-->
                    <material>
                        <ambient>0.0 0.0 1.0 1</ambient>
                        <diffuse>0.0 0.0 1.0 1</diffuse>
                        <specular>0.0 0.0 1.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <box>
                            <size>2.0 1.0 0.5</size>
                        </box>
                    </geometry>
                </collision>
            </link>
            
             <!--arbitrary link left -->
            <link name="left_frame_front">
                <pose relative_to="chassis">-0.7 0.4 -0.35 -1.5707 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                  <geometry>
                    <box>
                      <size>0.15 0.1 0.15</size>
                    </box>
                  </geometry>	
                </visual>
            </link>
            
            <!--arbitrary frame right-->
            <link name="right_frame_front">
                <pose relative_to="chassis">-0.7 -0.4 -0.35 1.5707 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                  <geometry>
                    <box>
                      <size>0.15 0.1 0.15</size>
                    </box>
                  </geometry>	
                </visual>
            </link>

            <!--Left wheel-->
            <link name='left_wheel'> 
                <pose relative_to="left_frame_front">0 0 0 0 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.14 0.14 0.14</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.1</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu>1</mu>
                                <mu2>10</mu2>
                            </ode>
                        </friction>
                    </surface>
                </collision>
            </link>

            <!--The same as left wheel but with different position-->
            <link name='right_wheel'>
                <pose relative_to="right_frame_front">0 0 0 0 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.14 0.14 0.14</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.1</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu>1</mu>
                                <mu2>10</mu2>
                            </ode>
                        </friction>
                    </surface>
                </collision>
            </link>
            
             <!--arbitrary link left -->
            <link name="left_frame">
                <pose relative_to="chassis">0.7 0.4 -0.35 -1.5707 0 0</pose> <!--angles are in radian-->
                <visual name='visual'>
                  <geometry>
                    <box>
                      <size>0.15 0.1 0.15</size>
                    </box>
                  </geometry>	
                </visual>
            </link>
            
            <!--arbitrary frame right-->
            <link name="right_frame">
                <pose relative_to="chassis">0.7 -0.4 -0.35 1.5707 0 0</pose> <!--angles are in radian-->
                <visual name='visual'>
                  <geometry>
                    <box>
                      <size>0.15 0.1 0.15</size>
                    </box>
                  </geometry>	
                </visual>
            </link>
            
             <!--arbitrary link left -->
            <link name="left_frame_back">
                <pose relative_to="chassis">0.7 0.4 -0.35 -1.5707 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                  <geometry>
                    <box>
                      <size>0.15 0.1 0.15</size>
                    </box>
                  </geometry>	
                </visual>
            </link>
            
            <!--arbitrary frame right-->
            <link name="right_frame_back">
                <pose relative_to="chassis">0.7 -0.4 -0.35 1.5707 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                  <geometry>
                    <box>
                      <size>0.15 0.1 0.15</size>
                    </box>
                  </geometry>	
                </visual>
            </link>
            
            <!--Left wheel-->
            <link name='left_wheel_back'> 
                <pose relative_to="left_frame">0 0 0 3.14 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>10</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.14 0.14 0.14</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.1</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu>0.9</mu>
                                <mu2>0.9</mu2>
                            </ode>
                        </friction>
                    </surface>
                </collision>
            </link>

            <!--The same as left wheel but with different position-->
            <link name='right_wheel_back'>
                <pose relative_to="right_frame">0 0 0 3.14 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>10</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.14 0.14 0.14</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.1</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu>0.9</mu>
                                <mu2>0.9</mu2>
                            </ode>
                        </friction>
                    </surface>
                </collision>
            </link>

            <!--left wheel joint-->
            <joint name='left_wheel_joint' type='revolute'>
                <pose relative_to='left_wheel'/>
                <parent>left_frame_front</parent>
                <child>left_wheel</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>

            <!--right wheel joint-->
            <joint name='right_wheel_joint' type='revolute'>
                <pose relative_to='right_wheel'/>
                <parent>right_frame_front</parent>
                <child>right_wheel</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>
            
            <!--left wheel ancker joint-->
            <joint name='left_anker_joint' type='revolute'>
                <pose relative_to='left_frame'/>
                <parent>left_frame_back</parent>
                <child>left_frame</child>
                <axis>
                    <xyz expressed_in='__model__'>0 0 1</xyz>
                    <limit>
                        <lower>-0.689775</lower>    <!--negative infinity-->
                        <upper>0.78535</upper>     <!--positive infinity-->
                        <effort>100</effort>
                    </limit>
                </axis>
            </joint>

            <!--right wheel joint ancker joint-->
            <joint name='right_anker_joint' type='revolute'>
                <pose relative_to='right_frame'/>
                <parent>right_frame_back</parent>
                <child>right_frame</child>
                <axis>
                    <xyz expressed_in='__model__'>0 0 1</xyz>
                    <limit>
                        <lower>-0.78535</lower>    <!--negative infinity-->
                        <upper>0.689775</upper>     <!--positive infinity-->
                        <effort>100</effort>
                    </limit>
                </axis>
            </joint>
            
            <!--left wheel back joint-->
            <joint name='left_wheel_back_joint' type='revolute'>
                <pose relative_to='left_wheel_back'/>
                <parent>left_frame</parent>
                <child>left_wheel_back</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>

            <!--right wheel back joint-->
            <joint name='right_wheel_back_joint' type='revolute'>
                <pose relative_to='right_wheel_back'/>
                <parent>right_frame</parent>
                <child>right_wheel_back</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>
            
            <!-- damper joints -->
            <joint name="j1" type="prismatic">
                <pose relative_to='chassis'/>
		    <parent>chassis</parent>
		    <child>left_frame_front</child>
		    <axis>
		    	<xyz>0 0 1</xyz>
			    <dynamics>
				    <spring_reference>-0.1</spring_reference>
				    <spring_stiffness>300</spring_stiffness>
				    <damping>10</damping>
			    </dynamics>
		    </axis>
	    </joint>
	    
	    <joint name="j2" type="prismatic">
                <pose relative_to='chassis'/>
            <parent>chassis</parent>
            <child>right_frame_front</child>
            <axis>
            	<xyz>0 0 1</xyz>
		    <dynamics>
			    <spring_reference>-0.1</spring_reference>
			    <spring_stiffness>300</spring_stiffness>
			    <damping>10</damping>
		    </dynamics>
            </axis>
	    </joint>
	    
	    <joint name="j3" type="prismatic">
                <pose relative_to='chassis'/>
            <parent>chassis</parent>
            <child>left_frame_back</child>
            <axis>
            	<xyz>0 0 1</xyz>
		    <dynamics>
			    <spring_reference>-0.1</spring_reference>
			    <spring_stiffness>300</spring_stiffness>
			    <damping>10</damping>
		    </dynamics>
            </axis>
	    </joint>
	    
	    <joint name="j4" type="prismatic">
                <pose relative_to='chassis'/>
            <parent>chassis</parent>
            <child>right_frame_back</child>
            <axis>
            	<xyz>0 0 1</xyz>
		    <dynamics>
			    <spring_reference>-0.1</spring_reference>
			    <spring_stiffness>300</spring_stiffness>
			    <damping>10</damping>
		    </dynamics>
            </axis>
	    </joint>
            
        <link name="camera">
            <pose relative_to='chassis'>0.95 0.05 1.35 0 0.3 0</pose>
            <inertial>
            <mass>0.1</mass>
            <inertia>
                <ixx>0.000166667</ixx>
                <iyy>0.000166667</iyy>
                <izz>0.000166667</izz>
            </inertia>
            </inertial>
            <collision name="collision">
            <geometry>
                <box>
                <size>0.1 0.1 0.1</size>
                </box>
            </geometry>
            </collision>
            <visual name="visual">
            <geometry>
                <box>
                <size>0.1 0.1 0.1</size>
                </box>
            </geometry>
            </visual>
            <sensor name="camera" type="camera">
            <camera>
                <horizontal_fov>1.047</horizontal_fov>
                <image>
                <width>1080</width>
                <height>810</height>
                </image>
                <clip>
                <near>0.1</near>
                <far>100</far>
                </clip>
            </camera>
            <always_on>1</always_on>
            <update_rate>30</update_rate>
            <visualize>true</visualize>
            <topic>camera</topic>
            </sensor>
	      </link>

        <link name="ultrasonic">
            <pose relative_to='chassis'>1 0 0 0 0 0</pose>
            <inertial>
            <mass>0.1</mass>
            <inertia>
                <ixx>0.000166667</ixx>
                <iyy>0.000166667</iyy>
                <izz>0.000166667</izz>
            </inertia>
            </inertial>
            <collision name="collision">
            <geometry>
                <box>
                <size>0.1 0.1 0.1</size>
                </box>
            </geometry>
            </collision>
            <visual name="visual">
            <geometry>
                <box>
                <size>0.1 0.1 0.1</size>
                </box>
            </geometry>
            </visual>
          <sensor name='gpu_lidar' type='gpu_lidar'>"
            <topic>lidar</topic>
            <update_rate>10</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>640</samples>
                        <resolution>1</resolution>
                        <min_angle>-0.3927</min_angle>
                        <max_angle>0.3927</max_angle>
                    </horizontal>
                    <vertical>
                        <samples>1</samples>
                        <resolution>0.01</resolution>
                        <min_angle>0</min_angle>
                        <max_angle>0</max_angle>
                    </vertical>
                </scan>
                <range>
                    <min>0.008</min>
                    <max>100.0</max>
                    <resolution>0.01</resolution>
                </range>
            </ray>
            <always_on>1</always_on>
            <visualize>true</visualize>
          </sensor>
        </link>
	      
	      <joint name='camera_joint' type='fixed'>
                <parent>chassis</parent>
                <child>camera</child>
              </joint>

        <joint name='sensor_joint' type='fixed'>
            <parent>chassis</parent>
            <child>ultrasonic</child>
        </joint>
        
        <plugin filename="SampleSystem"
            	      name="ignition::gazebo::systems::SampleSystem">
            	      <joint_name>right_wheel_joint</joint_name>
                      <joint_name_2>right_anker_joint</joint_name_2>
                      <joint_name_3>left_wheel_joint</joint_name_3>
                      <joint_name_4>left_anker_joint</joint_name_4>
                      <p_gain>10</p_gain>
                      <d_gain>5</d_gain>
                      <use_force_commands>false</use_force_commands>
    		</plugin>	
		
        </model>
        
        <model name='my_mesh'>
            <pose realitve_to="world">0 0 0 0 0 0</pose>
	      <static>1</static>
	      <link name='body'>
            <visual name='visual'>
            <geometry>
                <mesh>
                <uri>file://oval_strecke.dae</uri>
                </mesh>
            </geometry>
            </visual>
            <self_collide>0</self_collide>
            <enable_wind>0</enable_wind>
            <kinematic>0</kinematic>
	      </link>
    	</model>

        <plugin filename="libignition-gazebo-triggered-publisher-system.so"
		name="ignition::gazebo::systems::TriggeredPublisher">
	    <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
		<match field="data">16777235</match>
	    </input>
	    <output type="ignition.msgs.Double" topic="/set_motor_level_msg">
		data: 500
	    </output>
	</plugin>

	<plugin filename="libignition-gazebo-triggered-publisher-system.so"
		name="ignition::gazebo::systems::TriggeredPublisher">
