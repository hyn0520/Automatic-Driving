        <parent>chassis</parent>
        <child>imu</child>
    </joint>
    
    <plugin filename="SampleSystem"
                  name="ignition::gazebo::systems::SampleSystem">
                  <joint_name>right_wheel_joint</joint_name>
                  <joint_name_2>right_anker_joint</joint_name_2>
                  <joint_name_3>left_wheel_joint</joint_name_3>
                  <joint_name_4>left_anker_joint</joint_name_4>
                  <front_wheel_left>right_wheel_back_joint</front_wheel_left>
                  <front_wheel_right>left_wheel_back_joint</front_wheel_right>
                  <p_gain>0.01</p_gain>
