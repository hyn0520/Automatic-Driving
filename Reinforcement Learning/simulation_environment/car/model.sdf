    <link name="imu">
        <sensor name="imu" type="imu">
            <always_on>1</always_on>
            <update_rate>100</update_rate>
            <visualize>true</visualize>
            <topic>imu_data</topic>
          </sensor>
          <sensor name="magnetometer" type="magnetometer">
            <always_on>1</always_on>
            <update_rate>100</update_rate>
            <visualize>true</visualize>
            <topic>magnetometer</topic>
            <magnetometer>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.1</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.1</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.1</stddev>
                </noise>
              </z>
            </magnetometer>
          </sensor>
    </link>
      
      <joint name='camera_joint' type='fixed'>
            <parent>chassis</parent>
            <child>camera</child>
          </joint>

      
    <joint name='sensor_joint_front' type='fixed'>
        <parent>chassis</parent>
        <child>ultrasonic_front</child>
    </joint>
