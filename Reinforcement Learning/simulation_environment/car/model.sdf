<?xml version="1.0" ?>
<sdf version="1.5">
    <model name='vehicle_blue' canonical_link='chassis'>
        <pose relative_to='world'>0 3.8 0.01 0 0 0</pose>   <!--the pose is relative to the world by default-->
        
        <!--chassis-->
        <link name='chassis'>   
            <pose relative_to='__model__'>0.0 0 0.1 0 0 0</pose> 
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>4</mass>
                <inertia>
                    <ixx>4.00833e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>8.16666e-4</iyy>
                    <iyz>0</iyz>
                    <izz>3.46666e-3</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>file://simulation_projektseminar/old_car.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.3 0.2 0.09</size>
                    </box>
                </geometry>
            </collision>
        </link>

        <link name="back_box">
            <pose relative_to="chassis">-0.17 0.0 0.0</pose>
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>0.5</mass>
                <inertia>
                    <ixx>4.00833e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>8.08333e-4</iyy>
                    <iyz>0</iyz>
                    <izz>3.46666e-3</izz>
                </inertia>
            </inertial>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.04 0.20 0.09</size>
                    </box>
                </geometry>
            </collision>
        </link>

        <link name="top_box">
            <pose relative_to="chassis">0.065 0.0 0.115</pose>
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>0.3</mass>
                <inertia>
                    <ixx>7.2666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>2.30833e-3</iyy>
                    <iyz>0</iyz>
                    <izz>6.30833e-3</izz>
                </inertia>
            </inertial>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.09 0.26 0.14</size>
                    </box>
                </geometry>
            </collision>
        </link>

        <link name="front_bumper">
            <pose relative_to="chassis">0.2 0.0 -0.065</pose>
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>0.05</mass>
                <inertia>
                    <ixx>1.90833e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-4</iyy>
                    <iyz>0</iyz>
                    <izz>2.0083e-3</izz>
                </inertia>
            </inertial>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.04 0.15 0.02</size>
                    </box>
                </geometry>
            </collision>
        </link>
        
         <!--arbitrary link left -->
        <link name="left_frame_front">
            <pose relative_to="chassis">-0.145 0.1 -0.085 -1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>1.6666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-3</iyy>
                    <iyz>0</iyz>
                    <izz>1.6666e-3</izz>
                </inertia>
            </inertial>
        </link>
        
        <!--arbitrary frame right-->
        <link name="right_frame_front">
            <pose relative_to="chassis">-0.145 -0.1 -0.085 1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>1.6666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-3</iyy>
                    <iyz>0</iyz>
                    <izz>1.6666e-3</izz>
                </inertia>
            </inertial>
        </link>

        <!--Left wheel-->
        <link name='left_wheel'> 
            <pose relative_to="left_frame_front">0 0 0 0 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>1.895e-4</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.895e-4</iyy>
                    <iyz>0</iyz>
                    <izz>3.125e-4</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>file://wheel/wheel.dae</uri>
                        <scale>0.001 0.001 0.001</scale>
                      </mesh>
                </geometry>
                <pose>0 0 -0.02 0 -1.5707 -1.5707</pose>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.03</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.9</mu>
                            <mu2>10</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!--The same as left wheel but with different position-->
        <link name='right_wheel'>
            <pose relative_to="right_frame_front">0 0 0 0 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>1.895e-4</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.895e-4</iyy>
                    <iyz>0</iyz>
                    <izz>3.125e-4</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>file://wheel/wheel.dae</uri>
                        <scale>0.001 0.001 0.001</scale>
                      </mesh>
                </geometry>
                <pose>0 0 -0.02 0 -1.5707 -1.5707</pose>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.03</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.9</mu>
                            <mu2>10</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>
        
         <!--arbitrary link left -->
        <link name="left_frame">
            <pose relative_to="chassis">0.125 0.1 -0.085 -1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.1</mass>
                <inertia>
                    <ixx>1.6666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-3</iyy>
                    <iyz>0</iyz>
                    <izz>1.6666e-3</izz>
                </inertia>
            </inertial>
        </link>
        
        <!--arbitrary frame right-->
        <link name="right_frame">
            <pose relative_to="chassis">0.125 -0.1 -0.085 1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.1</mass>
                <inertia>
                    <ixx>1.6666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-3</iyy>
                    <iyz>0</iyz>
                    <izz>1.6666e-3</izz>
                </inertia>
            </inertial>
        </link>
        
         <!--arbitrary link left -->
        <link name="left_frame_back">
            <pose relative_to="chassis">0.125 0.1 -0.085 0 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.1</mass>
                <inertia>
                    <ixx>1.6666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-3</iyy>
                    <iyz>0</iyz>
                    <izz>1.6666e-3</izz>
                </inertia>
            </inertial>
        </link>
        
        <!--arbitrary frame right-->
        <link name="right_frame_back">
            <pose relative_to="chassis">0.125 -0.1 -0.085 0 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.1</mass>
                <inertia>
                    <ixx>1.6666e-3</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.6666e-3</iyy>
                    <iyz>0</iyz>
                    <izz>1.6666e-3</izz>
                </inertia>
            </inertial>
        </link>
        
        <!--Left wheel-->
        <link name='left_wheel_back'> 
            <pose relative_to="left_frame">0 0 0 3.14 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>1.895e-4</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.895e-4</iyy>
                    <iyz>0</iyz>
                    <izz>3.125e-4</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>file://wheel/wheel.dae</uri>
                        <scale>0.001 0.001 0.001</scale>
                      </mesh>
                </geometry>
                <pose>0 0 0.02 0 1.5707 -1.5707</pose>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.03</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.9</mu>
                            <mu2>10</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!--The same as left wheel but with different position-->
        <link name='right_wheel_back'>
            <pose relative_to="right_frame">0 0 0 3.14 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>1.895e-4</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1.895e-4</iyy>
                    <iyz>0</iyz>
                    <izz>3.125e-4</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>file://wheel/wheel.dae</uri>
                        <scale>0.001 0.001 0.001</scale>
                      </mesh>
                </geometry>
                <pose>0 0 0.02 0 1.5707 -1.5707</pose>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.03</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.9</mu>
                            <mu2>10</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!-- body joints-->
        <joint name="top_joint" type="fixed">
            <pose relative_to="chassis"/>
            <parent>chassis</parent>
            <child>top_box</child>
        </joint>

        <joint name="front_joint" type="fixed">
            <pose relative_to="chassis"/>
            <parent>chassis</parent>
            <child>front_bumper</child>
        </joint>

        <joint name="back_joint" type="fixed">
            <pose relative_to="chassis"/>
            <parent>chassis</parent>
            <child>back_box</child>
        </joint>

        <!--left wheel joint-->
        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel'/>
            <parent>left_frame_front</parent>
            <child>left_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>5</effort>
                </limit>
            </axis>
        </joint>

        <!--right wheel joint-->
        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel'/>
            <parent>right_frame_front</parent>
            <child>right_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>5</effort>
                </limit>
            </axis>
        </joint>
        
        <!--left wheel ancker joint-->
        <joint name='left_anker_joint' type='revolute'>
            <pose relative_to='left_frame'/>
            <parent>left_frame_back</parent>
            <child>left_frame</child>
            <axis>
                <xyz expressed_in='__model__'>0 0 1</xyz>
                <limit>
                    <lower>-0.689775</lower>    <!--negative infinity-->
                    <upper>0.78535</upper>     <!--positive infinity-->
                    <effort>100</effort>
                </limit>
            </axis>
        </joint>

        <!--right wheel joint ancker joint-->
        <joint name='right_anker_joint' type='revolute'>
            <pose relative_to='right_frame_back'/>
            <parent>right_frame_back</parent>
            <child>right_frame</child>
            <axis>
                <xyz expressed_in='__model__'>0 0 1</xyz>
                <limit>
                    <lower>-0.78535</lower>    <!--negative infinity-->
                    <upper>0.689775</upper>     <!--positive infinity-->
                    <effort>100</effort>
                </limit>
            </axis>
        </joint>
        
        <!--left wheel back joint-->
        <joint name='left_wheel_back_joint' type='revolute'>
            <pose relative_to='left_wheel_back'/>
            <parent>left_frame</parent>
            <child>left_wheel_back</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>5</effort>
                </limit>
            </axis>
        </joint>

        <!--right wheel back joint-->
        <joint name='right_wheel_back_joint' type='revolute'>
            <pose relative_to='right_wheel_back'/>
            <parent>right_frame</parent>
            <child>right_wheel_back</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>5</effort>
                </limit>
            </axis>
        </joint>
        
        <!-- damper joints -->
        <joint name="j1" type="prismatic">
            <pose relative_to='chassis'/>
        <parent>chassis</parent>
        <child>left_frame_front</child>
        <axis>
            <xyz>0 0 1</xyz>
            <dynamics>
                <spring_reference>0.0</spring_reference>
                <spring_stiffness>100000000</spring_stiffness>
                <damping>1500000</damping>
            </dynamics>
        </axis>
    </joint>
    
    <joint name="j2" type="prismatic">
            <pose relative_to='chassis'/>
        <parent>chassis</parent>
        <child>right_frame_front</child>
        <axis>
            <xyz>0 0 1</xyz>
        <dynamics>
            <spring_reference>0.0</spring_reference>
            <spring_stiffness>100000000</spring_stiffness>
            <damping>1500000</damping>
        </dynamics>
        </axis>
    </joint>
    
    <joint name="j3" type="prismatic">
            <pose relative_to='chassis'/>
        <parent>chassis</parent>
        <child>left_frame_back</child>
        <axis>
            <xyz>0 0 1</xyz>
        <dynamics>
            <spring_reference>0.0</spring_reference>
            <spring_stiffness>100000000</spring_stiffness>
            <damping>1500000</damping>
        </dynamics>
        </axis>
    </joint>
    
    <joint name="j4" type="prismatic">
            <pose relative_to='chassis'/>
        <parent>chassis</parent>
        <child>right_frame_back</child>
        <axis>
            <xyz>0 0 1</xyz>
        <dynamics>
            <spring_reference>0.0</spring_reference>
            <spring_stiffness>100000000</spring_stiffness>
            <damping>1500000</damping>
        </dynamics>
        </axis>
    </joint>
        
    <link name="camera">
        <pose relative_to="top_box">0.1 0 0.074 0 0.62 0</pose>
        <sensor name="camera" type="camera">
            <camera>
                <horizontal_fov>1.3</horizontal_fov>
                <image>
                <width>640</width>
                <height>480</height>
                </image>
                <clip>
                <near>0.1</near>
                <far>100</far>
                </clip>
            </camera>
            <always_on>1</always_on>
            <update_rate>30</update_rate>
            <visualize>true</visualize>
            <topic>camera</topic>
        </sensor>
      </link>
    
    <link name="ultrasonic_front">
        <pose relative_to="chassis">0.17 0 0 0 0 0</pose>
      <sensor name='gpu_lidar' type='gpu_lidar'>
        <topic>lidar_front</topic>
        <update_rate>10</update_rate>
        <ray>
            <scan>
                <horizontal>
                    <samples>10</samples>
                    <resolution>1</resolution>
                    <min_angle>-0.3927</min_angle>
                    <max_angle>0.3927</max_angle>
                </horizontal>
                <vertical>
                    <samples>1</samples>
                    <resolution>0.01</resolution>
                    <min_angle>0</min_angle>
                    <max_angle>0</max_angle>
                </vertical>
            </scan>
            <range>
                <min>0.01</min>
                <max>20.0</max>
                <resolution>0.01</resolution>
            </range>
        </ray>
        <always_on>1</always_on>
      </sensor>
      <visual name='visual'>
        <geometry>
          <box>
                <size>0.1 0.1 0.1</size>
                </box>
        </geometry>
            </visual>
    </link>
